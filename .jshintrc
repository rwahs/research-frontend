{
  // settings
  "maxdepth": 5,
  "quotmark": true,
  "strict": true,

  // restrictions
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "immed": true,
  "latedef": true,
  "newcap": true,
  "nonbsp": true,
  "nonew": true,
  "notypeof": true,
  "singleGroups": false,
  "undef": true,

  // environment
  "debug": true,
  "devel": true,

  // globals
  "browser": true,
  "globals": {
    "require": true,
    "define": true
  }
}
